{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample server for RESTful API.",
        "title": "Swagger API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost",
    "basePath": "/",
    "paths": {
        "/": {
            "get": {
                "description": "返回 \"Hello World\" 字符串",
                "tags": [
                    "example"
                ],
                "summary": "Say Hello World",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "返回一个 json 格式的数据库健康检查信息",
                "tags": [
                    "example"
                ],
                "summary": "返回数据库健康检查",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    }
}